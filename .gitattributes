pubspec.yaml filter=lfs diff=lfs merge=lfs -text
CHANGELOG.md filter=lfs diff=lfs merge=lfs -text
README.md filter=lfs diff=lfs merge=lfs -text
media filter=lfs diff=lfs merge=lfs -text
pubspec.lock filter=lfs diff=lfs merge=lfs -text
include filter=lfs diff=lfs merge=lfs -text
